model Animepahe {
  id             String              @id
  alId           Int?
  title          String?
  image          String?
  cover          String?
  hasSub         Boolean?
  externalLinks  Json?
  status         String?
  type           String?
  releaseDate    String?
  totalEpisodes  Int?
  episodePages   Int?

  episodes       Json?
}

model AnimepaheEpisode {
  id          String   @id
  number      Int?
  title       String?
  image       String?
  duration    String?
  url         String?
}

model AnimepaheExternalLink {
  id          String   @id
  url         String
  sourceName  String
}