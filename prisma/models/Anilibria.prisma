  alternative String?

  /// @DtoRelationCanConnectOnCreate
  anilibria Anilibria @relation("AnilibriaName", fields: [anilibriaId], references: [id])
}

model AnilibriaSeason {
  /// @DtoReadOnly
  /// @DtoCreateOptional
  /// @DtoUpdateOptional
  id          Int     @id @default(autoincrement())
  anilibriaId Int     @unique
  value       String?
